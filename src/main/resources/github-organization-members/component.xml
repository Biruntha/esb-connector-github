<component name="github-organization-members" type="synapse/template">
    <subComponents>
        <component name="getMembersList">
            <file>getMembersList.xml</file>
            <description>List all users who are members of an organization.</description>
        </component>
        <component name="AddMembership">
            <file>AddMembership.xml</file>
            <description>To add someone as a member to an organization.</description>
        </component>
